{
  "manifest_version": 2,

  "name": "Facebook Incognito",
  "description": "Removes the Facebook overlay which prompts for login",
  "version": "0.1",
  "homepage_url": "https://github.com/goncalor/facebook-incognito",

  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*", "http://www.facebook.com/*"],
      "js": ["script.js"]
    }
  ]
}
